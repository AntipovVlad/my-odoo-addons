<odoo>
    <data>
        <record id="view_product_product_form" model="ir.ui.view">
            <field name="name">product.product.set.form</field>
            <field name="model">product.product.set</field>
            <field name="arch" type="xml">
                <form string="arch">
                    <sheet>
                        <group>
                            <field name="set_id" can_create="true" can_write="true"/>
                            <field name="item_id"/>
                            <field name="quantity"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>